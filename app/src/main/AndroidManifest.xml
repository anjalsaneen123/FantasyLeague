<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.fantasyleaguefm.fl">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fl"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LeagueActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TableActivity" />
        <activity android:name=".Teams.Arsenal" />
        <activity android:name=".Teams.Middlesborough" />
        <activity android:name=".Teams.Burnley" />
        <activity android:name=".Teams.Boro" />
        <activity android:name=".Teams.Chelsea" />
        <activity android:name=".Teams.CP" />
        <activity android:name=".Teams.Everton" />
        <activity android:name=".Teams.Hull" />
        <activity android:name=".Teams.Liverpool" />
        <activity android:name=".Teams.Lexter" />
        <activity android:name=".Teams.United" />
        <activity android:name=".Teams.ManCity"></activity>
    </application>

</manifest>